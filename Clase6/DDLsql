DROP TABLE EMPLEADOS;
DROP TABLE GENEROS;

CREATE TABLE EMPLEADOS(
  Identificacion VARCHAR2(60),
  Nombre_Completo VARCHAR2(60),
  Fecha_Nacimiento DATE,
  Genero INTEGER
) TABLESPACE CLASE6;

CREATE TABLE GENEROS(
  Identificacion INTEGER,
  Descripcion VARCHAR2(60)
) TABLESPACE CLASE6;
